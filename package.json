{
  "name": "student-enrollment-system",
  "version": "1.0.0",
  "description": "Backend system for student course enrollment platform",
  "main": "server.js",
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --env-file=.env --watch server.js",
    "dev:nodemon": "nodemon --env-file=.env server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:setup": "node --env-file=.env scripts/setup-test-db.js",
    "test:all": "yarn test:setup && yarn test",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node --env-file=.env prisma/seed.js",
    "setup:db": "node --env-file=.env scripts/setup-db.js",
    "generate:admin": "node scripts/generate-admin.js"
  },
  "keywords": [
    "student",
    "enrollment",
    "course",
    "management",
    "api",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "3.6.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=22.16.0 <23.0.0",
    "npm": ">=10.0.0"
  }
}
